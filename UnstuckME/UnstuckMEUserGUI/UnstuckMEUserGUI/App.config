<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://localhost:9000/UnstuckMEService"
          binding="netTcpBinding" bindingConfiguration="IncreaseMaxBuffer_and_MessageSize" contract="UnstuckMEInterfaces.IUnstuckMEService"
          name="UnstuckMEServiceEndPoint" kind="" endpointConfiguration="" />
    </client>
    <bindings>
      <netTcpBinding>
        <binding name="IncreaseMaxBuffer_and_MessageSize" maxBufferSize="65536" maxReceivedMessageSize="65536" />
        <!--if we increase these values past 524288, we have to readjust the MaxBufferPoolSize property, as this is it's default value-->
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>


