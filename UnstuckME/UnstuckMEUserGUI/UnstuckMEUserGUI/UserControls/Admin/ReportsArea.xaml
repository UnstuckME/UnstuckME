<UserControl x:Class="UnstuckMEUserGUI.UserControls.Admin.ReportsArea"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UnstuckMEUserGUI.UserControls.Admin"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Border x:Name="ResolveReportsBtn" Grid.Row="1" Cursor="Hand" MouseLeftButtonDown="ResolveReportsBtn_Click" Height="30" Width="111" CornerRadius="5,5,5,5" Background="#336699" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="0,3,0,2.8" BorderBrush="Black" BorderThickness="1,1,1,1">
            <Label Content="Resolve Issues" Foreground="White" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"></Label>
        </Border>
    </Grid>
</UserControl>
