<UserControl x:Class="UnstuckMEUserGUI.TutorStickerSubmit"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UnstuckMEUserGUI"
             mc:Ignorable="d" 
             d:DesignHeight="35" d:DesignWidth="250">
    <Grid Background="Gray">
        <Border BorderBrush="Black" BorderThickness="1,1,1,1"></Border>
        <TextBox x:Name="textBoxOrgName" Width="209" Cursor="Arrow"  HorizontalAlignment="Left" Text="Tutoring Organization" Padding="5,0,0,0" Foreground="White" VerticalContentAlignment="Center" TextWrapping="NoWrap" IsReadOnly="True" IsReadOnlyCaretVisible="False" Background="{x:Null}" BorderBrush="{x:Null}" SelectionBrush="{x:Null}" BorderThickness="0"></TextBox>
        <Button x:Name="buttonRemoveOrg" Cursor="Hand" Click="buttonRemoveOrg_Click" HorizontalAlignment="Right" Background="{x:Null}" VerticalAlignment="Center" Width="35" Height="35" BorderBrush="{x:Null}" BorderThickness="0">
            <Image Source="/Resources/Xmark/XmarkRed.png" Height="25" HorizontalAlignment="Center" VerticalAlignment="Center"></Image>
        </Button>
    </Grid>
</UserControl>
