<Window x:Class="UnstuckMEServerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UnstuckMEServerWPF"
        mc:Ignorable="d"
        Title="UnstuckME Server" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="emailAddressTextBox" HorizontalAlignment="Left" Height="21" Margin="200,155,0,0" TextWrapping="Wrap" Text="Email Address" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="200,196,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonLogin" Content="Login" HorizontalAlignment="Left" Margin="223,237,0,0" VerticalAlignment="Top" Width="75" Click="buttonLogin_Click"/>

    </Grid>
</Window>
