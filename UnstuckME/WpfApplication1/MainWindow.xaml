<Window x:Class="UnstuckMEServerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UnstuckMEServerWPF"
        mc:Ignorable="d"
        Title="UnstuckME Server" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="emailAddressTextBox" HorizontalAlignment="Center" Height="22" Margin="200,154,197,143" TextWrapping="Wrap" Text="Email Address" VerticalAlignment="Center" Width="120" FontStyle="Italic" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Center" Margin="200,198,197,103" VerticalAlignment="Center" Width="120"/>
        <Button x:Name="buttonLogin" Content="Login" HorizontalAlignment="Left" Margin="223,237,0,0" VerticalAlignment="Top" Width="75" Click="buttonLogin_Click"/>
        <Button x:Name="button_CreateNewUser" Content="Create New User" HorizontalAlignment="Left" Margin="200,276,0,0" VerticalAlignment="Top" Width="120" Background="White" BorderBrush="White" Foreground="#FF0B37FF" Click="button_CreateNewUser_Click"/>

    </Grid>
</Window>
